<?xml version="1.0"?>
<object name="skill" meta_type="Dexterity FTI" i18n:domain="plone"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Skill</property>
 <property name="description"
    i18n:translate="">Skill, grouped by group within expertise.</property>
 <property name="icon_expr">string:${portal_url}/document_icon.png</property>
 <property name="factory">skill</property>
 <property name="add_view_expr">string:${folder_url}/++add++skill</property>
 <property name="link_target"></property>
 <property name="immediate_view">skill</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <property name="default_view">skill</property>
 <property name="view_methods">
  <element value="skill"/>
 </property>
 <property name="default_view_fallback">False</property>
 <property name="add_permission">cmf.AddPortalContent</property>
 <property name="klass">plone.dexterity.content.Container</property>
 <property name="behaviors">
  <element value="plone.app.dexterity.behaviors.metadata.IDublinCore"/>
  <element value="plone.app.content.interfaces.INameFromTitle"/>
  <element
     value="collective.dexteritytextindexer.behavior.IDexterityTextIndexer"/>
  <element
     value="plone.app.dexterity.behaviors.exclfromnav.IExcludeFromNavigation"/>
  <element
     value="plone.app.dexterity.behaviors.nextprevious.INextPreviousEnabled"/>
  <element
     value="plone.app.dexterity.behaviors.nextprevious.INextPreviousToggle"/>
  <element
     value="plone.app.referenceablebehavior.referenceable.IReferenceable"/>
  <element value="plone.app.versioningbehavior.behaviors.IVersionable"/>
 </property>
 <property name="schema"></property>
 <property
    name="model_source">&lt;model 
    xmlns:security="http://namespaces.plone.org/supermodel/security" 
    xmlns:lingua="http://namespaces.plone.org/supermodel/lingua" 
    xmlns:marshal="http://namespaces.plone.org/supermodel/marshal" 
    xmlns:form="http://namespaces.plone.org/supermodel/form" 
    xmlns:indexer="http://namespaces.plone.org/supermodel/indexer" 
    xmlns="http://namespaces.plone.org/supermodel/schema"&gt;
  &lt;schema&gt;
    &lt;field name="text" type="plone.app.textfield.RichText"&gt;
      &lt;description/&gt;
      &lt;required&gt;False&lt;/required&gt;
      &lt;title&gt;Text&lt;/title&gt;
    &lt;/field&gt;
    &lt;field name="group" type="zope.schema.Choice"&gt;
      &lt;description&gt;Pick the group that applies to this Skill.&lt;/description&gt;
      &lt;title&gt;Group&lt;/title&gt;
      &lt;vocabulary&gt;vs.knowledge.GroupsVocabularyFactory&lt;/vocabulary&gt;
    &lt;/field&gt;
    &lt;fieldset name="values" label="Values"&gt;
      &lt;field name="member_level_show" type="zope.schema.List"&gt;
        &lt;description&gt;Not meant for immediate editing. Only in case of emergency. Go through the available edit screens.&lt;/description&gt;
        &lt;required&gt;False&lt;/required&gt;
        &lt;title&gt;Member | Level | Show in profile&lt;/title&gt;
        &lt;value_type type="zope.schema.TextLine"/&gt;
      &lt;/field&gt;
    &lt;/fieldset&gt;
  &lt;/schema&gt;
&lt;/model&gt;
 </property>
 <property name="model_file"></property>
 <property name="schema_policy">dexterity</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    description="" icon_expr="" link_target="" url_expr="string:${object_url}"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
